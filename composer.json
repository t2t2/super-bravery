{
	"name": "t2t2/super-bravery",
	"description": "The Laravel Lumen Framework.",
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/lumen-framework": "5.1.*",
		"vlucas/phpdotenv": "~1.0",
		"guzzlehttp/guzzle": "~6.0",
		"guzzlehttp/promises": "~1.0.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"fzaninotto/faker": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"t2t2\\SuperBravery\\": "app/"
		},
		"files": [
			"app/helpers.php"
		],
		"classmap": [
			"database/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
